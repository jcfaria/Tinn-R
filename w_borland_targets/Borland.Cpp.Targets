			GenerateLibFile="$(ILINK_GenerateLibFile)"
			AllWarnings="$(ILINK_AllWarnings)"
			DisableWarnings="$(ILINK_DisableWarnings)"
			SelectedWarnings="$(ILINK_SelectedWarnings)"
			wexp="$(ILINK_wexp)"
			wrty="$(ILINK_wrty)"
